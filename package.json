    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
